Print
	maim pic-full-$(date '+%y%m%d-%H%M-%S').png
shift + Print
	maimpick Recording
super + Print
	maim -s pic-region-$(date '+%y%m%d-%H%M-%S').png
super + p
	rofi_custom -show combi -modi combi,run,ssh,window -combi-modi run,ssh,window
super + shift + p
	rofi-gopass
super + ctrl + p
	power_menu
super + Return
	kitty
super + shift + q
	killall Xorg
super + shift + c
	bspc node -c

super + e
	rofi-surfraw

super + s
	bspc node -g sticky && bspc node -g locked && bspc node -g private
super + shift + s
	pip_focused

super + f
	bspc node -t \~fullscreen

XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle
XF86Audio{Raise,Lower}Volume
	pactl set-sink-volume @DEFAULT_SINK@ {+,-}1000
XF86MonBrightnessDown
	light -U 5%
XF86MonBrightnessUp
	light -A 5%

super + space
      	bspc node -t "~floating"
super + {grave,Tab}
	bspc {window,desktop} -f last

super + shift + Return
	bspc node -s "last.local.!focused"

super + ctrl {h,j,k,l}
    bspc node -z {left -20 0,bottom 0 20,top 0 -20,right 20 0}
super + shift + {h,j,k,l}
    bspc node -z {right -20 0,top 0 20,bottom 0 -20,left 20 0}

super + ampersand
	bspc desktop -f I
super + bracketleft
	bspc desktop -f II
super + braceleft
	bspc desktop -f III
super + braceright
	bspc desktop -f IV
super + parenleft
	bspc desktop -f V
super + equal
	bspc desktop -f VI
super + asterisk
	bspc desktop -f VII
super + parenright
	bspc desktop -f VIII
super + plus
	bspc desktop -f IX

super + shift + ampersand
	bspc node -d I
super + shift + bracketleft
	bspc node -d II
super + shift + braceleft
	bspc node -d II
super + shift + braceright
	bspc node -d IV
super + shift + parenleft
	bspc node -d V
super + shift + equal
	bspc node -d VI
super + shift + asterisk
	bspc node -d VII
super + shift + parenright
	bspc node -d VIII
super + shift + plus
	bspc node -d IX
